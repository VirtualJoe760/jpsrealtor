{
  "drop_cowboy": {
    "name": "Drop Cowboy RVM",
    "description": "Ringless voicemail platform for real estate agents",
    "requiredFields": ["Phone"],
    "mapping": {
      "First Name": ["firstName", "first_name", "fname", "First Name"],
      "Last Name": ["lastName", "last_name", "lname", "Last Name"],
      "Phone": ["_cleanedPhone"],
      "Email": ["email", "email_address", "Email"],
      "Address": ["address", "street", "Address", "Street Address"],
      "City": ["city", "City"],
      "State": ["state", "province", "State"],
      "ZIP": ["zip", "zipCode", "postal_code", "ZIP", "Zip Code"]
    },
    "phoneFormat": "national",
    "defaultCountryCode": "US"
  },
